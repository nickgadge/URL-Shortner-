<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>DLink</title>
    <link rel="stylesheet" href="/styles.css">
</head>

<body class="index-page">

    <header style="background: linear-gradient(90deg, #eef2f4, #e0ebf2, #e1ecf3, #e3eef3, #ecf1f4);";>
        <h1 ><a href="/"style="color: #ecf1f4;">D Link</a></h1>
        <nav>
            <% if (user) { %>
                <a style="display: none;" href="/dashboard">Dashboard</a>
                <a style="display: none;" href="/auth/logout">Logout</a>
            <% } else { %>
                <a style="display: none;" href="/auth/login">Login</a>
                <a style="display: none;" href="/auth/register">Register</a>
            <% } %>
        </nav>
    </header>
    

    <div class="main-container">
        <script type="module" src="https://unpkg.com/@splinetool/viewer@1.9.32/build/spline-viewer.js" async></script>
        <spline-viewer loading-anim-type="spinner-small-light" url="https://prod.spline.design/Xz-viGHSKsAGfOaG/scene.splinecode"><img src="data:image/png;base64," alt="Spline preview" style="width: 100%; height: 100%;"/></spline-viewer>

        <div class="button-container">
            <% if (user) { %>
                <a class="btn" href="/dashboard">Dashboard</a>
                <a class="btn" href="/auth/logout">Logout</a>
            <% } else { %>
                <a class="btn" href="/auth/login">Login</a>
                <a class="btn" href="/auth/register">Register</a>
            <% } %>
        </div>
    </div>
</body>
